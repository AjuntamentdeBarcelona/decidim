<%
@title = "Proceso -- Decidim Barcelona"
@activesection = "processes"
@activepage = "info"
%>

<%= partial "template_top" %>

<main>
  <%= partial "d_module_header" %>
  <div class="wrapper">

    <div class="row column">
      <h2 class="section-heading">89 Proposals</h2>
    </div>
    <div class="row small-up-1 medium-up-2 mediumlarge-up-2 large-up-3 card-grid">
      <% 3.times do %>
        <div class="column">
          <%= partial "d_card" %>
        </div>
      <% end %>
    </div>

    <div class="row column">
      <h2 class="section-heading">15 Meetings</h2>
    </div>
    <div class="row small-up-1 medium-up-2 mediumlarge-up-2 large-up-3 card-grid">
      <% 3.times do %>
        <div class="column">
          <%= partial "d_card" %>
        </div>
      <% end %>
    </div>


    <div class="row column">
      <h2 class="section-heading">Stats</h2>
    </div>
    <div class="row small-up-1 medium-up-2 mediumlarge-up-2 large-up-3 card-grid">
      <div class="column">
        <%= partial "d_stats" %>
      </div>
    </div>

  </div>
</main>

<%= partial "template_bottom" %>